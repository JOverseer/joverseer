<launch4jConfig>

  <dontWrapJar>true</dontWrapJar>
  <headerType>gui</headerType>
  <jar>joverseer.jar</jar>
  <outfile>dist/jOverseer.exe</outfile>
  <errTitle>jOverseer installer</errTitle>
  <cmdLine></cmdLine>
  <chdir></chdir>
  <priority>normal</priority>
  <downloadUrl>https://adoptium.net/en-GB/temurin/releases?version=11</downloadUrl>
  <supportUrl></supportUrl>
  <stayAlive>false</stayAlive>
  <manifest></manifest>
  <icon>../dist/joverseer.ico</icon>
  <jre>
    <path>jre</path>
    <bundledJre64Bit>false</bundledJre64Bit>
    <minVersion>1.8.0</minVersion>
    <maxVersion>22.0.1</maxVersion>
    <jdkPreference>preferJre</jdkPreference>
    <maxHeapSize>512</maxHeapSize>
    <opt>-Djava.net.preferIPv4Stack=true</opt>
    <opt>-Dsun.java2d.dpiaware=false</opt>
    <opt>--add-exports java.desktop/com.sun.java.swing.plaf.windows=ALL-UNNAMED</opt>
  </jre>
  <versionInfo>
    <fileVersion>1.0.0.0</fileVersion>
    <txtFileVersion>1.0.0.0</txtFileVersion>
    <fileDescription>jOverseer launcher</fileDescription>
    <copyright>Middle-Earth Games Ltd</copyright>
    <productVersion>1.18.19.0</productVersion>
    <txtProductVersion>1.18.19.0</txtProductVersion>
    <productName>jOverseer</productName>
    <companyName>Middle Earth Games</companyName>
    <internalName>jOverseer</internalName>
    <originalFilename>jOverseer.exe</originalFilename>
  </versionInfo>

</launch4jConfig>
